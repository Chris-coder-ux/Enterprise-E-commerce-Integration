{
  "env": {
    "browser": true,
    "jquery": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "script"
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "no-console": ["warn", { "allow": ["error"] }],
    "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
    "prefer-optional-chain": "off",
    "prefer-const": "warn",
    "no-var": "error",
    "object-shorthand": ["warn", "always"],
    "quote-props": ["warn", "as-needed"],
    "no-undef": "error"
  },
  "globals": {
    "wp": "readonly",
    "ajaxurl": "readonly",
    "miIntegracionApi": "readonly",
    "mia_admin_ajax": "readonly",
    "miEndpointsPage": "readonly",
    "miApiCacheAdmin": "readonly",
    "miApiCompat": "readonly",
    "mi_hpos_test": "readonly",
    "gridjs": "readonly"
  }
}